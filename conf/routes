
GET         /                       controllers.HomeController.index

GET         /dictionary             controllers.DictionaryController.getExcelDictionary

GET         /json/:pageIndex        controllers.DictionaryController.getJsonPage(pageIndex: Integer)

POST        /upload                 controllers.DictionaryController.uploadText

# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file           controllers.Assets.versioned(path="/public", file: Asset)
